<launch>
  <!-- Launch the scan_to_scan_filter_chain node -->
  <node pkg="laser_filters" type="scan_to_scan_filter_chain" name="scan_filter" output="screen">
    <!-- Load the filter configuration from the YAML file -->
    <rosparam command="load" file="$(find nav_test)/param/scan_filter.yaml" />
  </node>
</launch>